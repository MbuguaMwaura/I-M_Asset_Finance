        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.PreviewActivity"></activity>
        <activity android:name=".ui.DjangoFormTwoActivity" />
        <activity android:name=".ui.DjangoFormActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.DeclarationActivity" />
        <activity android:name=".ui.AttachmentsActivity" />
        <activity android:name=".ui.CreditActivity" />
        <activity android:name=".ui.AssetDetail" />
        <activity android:name=".ui.SupplierActivity" />
        <activity android:name=".ui.AdditionalInfoCompany" />
        <activity android:name=".ui.AdditionalInformation" />
        <activity android:name=".ui.ExistingAssetsActivity" />
        <activity android:name=".ui.BankDetailsActivity" />
